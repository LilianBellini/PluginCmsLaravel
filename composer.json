{
    "name": "lilianbellini/plugincmslaravel",
    "description": "A CMS plugin for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lilian Bellini",
            "email": "lilian@flowpi.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/sanctum": "^4.0",
        "openai-php/laravel": "^0.10.2",
        "google/apiclient": "^2.18"
    },
    "require-dev": {
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "LilianBellini\\PluginCmsLaravel\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:LilianBellini/PluginCmsLaravel.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "LilianBellini\\PluginCmsLaravel\\PluginCmsLaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
